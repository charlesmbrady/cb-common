{
  "name": "obxsurfinformation",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/obxsurfinformation",
  "projectType": "application",
  "tags": ["app", "next"],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/obxsurfinformation",
        "index": "apps/obxsurfinformation/src/index.html",
        "baseHref": "/",
        "main": "apps/obxsurfinformation/src/main.tsx",
        "assets": [
          "apps/obxsurfinformation/src/assets",
          {
            "input": "libs/ui-tailwind/src/assets",
            "glob": "**/*",
            "output": "/"
          }
        ],
        "styles": [],
        "scripts": []
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
        "options": {
          "jestConfig": "apps/obxsurfinformation/jest.config.ts"
        }
      }
    }
  }
}
